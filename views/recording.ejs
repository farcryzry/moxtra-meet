<script type="text/javascript" src="https://www.grouphour.com/api/js/moxtra-latest.js" id="moxtrajs"
        data-client-id="lu9gyOkcLX0"></script>
<!--<script type="text/javascript" src="/javascripts/moxtra-latest.grouphour.js" id="moxtrajs" data-client-id="lu9gyOkcLX0"></script>-->

<script type="text/javascript" src="/javascripts/meet.js"></script>

<script type="text/javascript">

    var meetId = "<%=meetId%>";

    $(function () {
        maodou.getToken({id: 123456}, function (user, access_token) {
            $.get('/rest/' + meetId + '/' + access_token, function (data) {
                console.log(data);
                if (data.data != null) {
                    if (data.data.recordings != null && data.data.recordings.length > 0) {
                        //$('.recordings').append(JSON.stringify(data.data.recordings));
                        $('#recording').attr('src', data.data.recordings[0].download_url);
                    } else {
                        alert($.t("recordingNotReady"));
                    }
                }
            });
        });
    });
</script>
</body>
</html>